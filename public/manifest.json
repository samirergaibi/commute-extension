{
  "manifest_version": 3,
  "name": "Commute Helper",
  "description": "Save and quickly view your regular commuting routes in Sweden",
  "version": "0.0.2",
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "icon-16.png",
      "32": "icon-32.png",
      "48": "icon-48.png",
      "128": "icon-128.png"
    }
  },
  "permissions": ["storage"],
  "options_page": "options/index.html",
  "icons": {
    "16": "icon-16.png",
    "32": "icon-32.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  }
}
